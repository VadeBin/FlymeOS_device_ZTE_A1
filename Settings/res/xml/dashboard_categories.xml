<?xml version="1.0" encoding="utf-8"?>
<dashboard-categories
  xmlns:android="http://schemas.android.com/apk/res/android">
    <dashboard-category android:id="@id/wireless_section" android:title="@string/header_category_wireless_networks">
        <dashboard-tile android:icon="@drawable/ic_sim_sd" android:id="@id/network_settings" android:title="@string/dual_sim_network_settings_title" android:fragment="com.android.settings.MobileNetworkSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_wireless" android:id="@id/wifi_settings" android:title="@string/wifi_settings_title" android:fragment="com.android.settings.wifi.WifiSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_bluetooth2" android:id="@id/bluetooth_settings" android:title="@string/bluetooth_settings_title" android:fragment="com.android.settings.bluetooth.BluetoothSettings" />
        <dashboard-tile android:icon="@drawable/ic_sim_sd" android:id="@id/sim_settings" android:title="@string/sim_settings_title" android:fragment="com.android.settings.sim.SimSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_data_usage" android:id="@id/data_usage_settings" android:title="@string/data_usage_summary_title" android:fragment="com.android.settings.DataUsageSummary" />
        <dashboard-tile android:id="@id/operator_settings" android:fragment="com.android.settings.WirelessSettings">
            <intent android:action="com.android.settings.OPERATOR_APPLICATION_SETTING" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_more" android:id="@id/wireless_settings" android:title="@string/connectivity_others" android:fragment="com.android.settings.WirelessSettings" />
    </dashboard-category>
    <dashboard-category android:id="@id/personal_section" android:title="@string/header_category_notificaion">
        <dashboard-tile android:icon="@drawable/ic_settings_notifications" android:id="@id/notification_settings" android:title="@string/notification_and_statusbar" android:fragment="com.meizu.settings.notificationstatusbar.NotificationStatusbarSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_sound" android:id="@id/sound_settings" android:title="@string/sound_and_vibrate_title" android:fragment="com.meizu.settings.soundandvibrate.SoundAndVibrateSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_dnd" android:id="@id/do_not_disturb" android:title="@string/do_not_disturb_title" android:fragment="com.meizu.settings.donotdisturb.MzDonotDisturbPreferenceFragment" />
        <dashboard-tile android:icon="@drawable/ic_settings_home" android:id="@id/home_settings" android:title="@string/home_settings" android:fragment="com.android.settings.HomeSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_storage" android:id="@id/storage_settings" android:title="@string/storage_settings" android:fragment="com.android.settings.deviceinfo.Memory" />
        <dashboard-tile android:icon="@drawable/ic_settings_multiuser" android:id="@id/user_settings" android:title="@string/user_settings_title" android:fragment="com.android.settings.users.UserSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_nfc_payment" android:id="@id/nfc_payment_settings" android:title="@string/nfc_payment_settings_title" android:fragment="com.android.settings.nfc.PaymentSettings" />
        <dashboard-tile android:id="@id/manufacturer_settings" android:fragment="com.android.settings.WirelessSettings">
            <intent android:action="com.android.settings.MANUFACTURER_APPLICATION_SETTING" />
        </dashboard-tile>
    </dashboard-category>
    <dashboard-category android:id="@id/system_section" android:title="@string/header_category_system">
        <dashboard-tile android:icon="@drawable/ic_settings_theme" android:id="@id/desktop_wallpaper_settings" android:title="@string/custom_settings_title" android:fragment="com.meizu.settings.customise.MzWallpaperTypeSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_easymode" android:id="@id/easy_mode" android:title="@string/easy_mode" />
        <dashboard-tile android:icon="@drawable/ic_settings_display" android:id="@id/display_settings" android:title="@string/diaplay_and_brightness" android:fragment="com.meizu.settings.display.MzDisplaySettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_security" android:id="@id/security_settings" android:title="@string/security_settings_title" android:fragment="com.meizu.settings.security.SecuritySettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_lock_fp" android:title="@string/fp_title">
            <intent android:targetPackage="com.zte.fingerprints" android:action="com.zte.fingerprint.main.FingerSettingActivity" android:targetClass="com.zte.fingerprint.main.FingerSettingActivity" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_language" android:id="@id/language_settings" android:title="@string/language_time" android:fragment="com.meizu.settings.language.LanguageAndDatetimeSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_accessibility" android:id="@id/accessibility_settings" android:title="@string/accessibility_settings" android:fragment="com.meizu.settings.accessibility.MZAccessibilitySettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_dhj"  android:title="@string/keyexchange_title">
            <intent android:action="com.android.keyexchange.EXCHANGE_KEY" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_djdz" android:title="@string/gesture_title">
            <intent android:targetPackage="com.zte.zgesture" android:action="com.zte.zgesture.MainSettingActivity" android:targetClass="com.zte.zgesture.MainSettingActivity" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_backup" android:id="@id/privacy_settings" android:title="@string/privacy_settings" android:fragment="com.android.settings.PrivacySettings" />
    </dashboard-category>
    <dashboard-category android:id="@id/account_section" android:title="@string/account_settings_title">
        <dashboard-tile android:icon="@drawable/ic_settings_flyme_account" android:id="@id/flyme_account" android:title="@string/flyme_account">
            <intent android:action="com.meizu.account.ACCOUNTCENTER" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_accounts" android:id="@id/account_settings" android:title="@string/other_account" android:fragment="com.android.settings.accounts.AccountSettings" />
    </dashboard-category>
    <dashboard-category android:id="@id/device_section" android:title="@string/header_category_device">
        <dashboard-tile android:icon="@drawable/ic_settings_battery" android:id="@id/battery_settings" android:title="@string/power_usage_summary_title" android:fragment="com.meizu.settings.fuelgauge.BatteryManagementMain" />
        <dashboard-tile android:icon="@drawable/ic_settings_location" android:id="@id/location_settings" android:title="@string/location_settings_title" android:fragment="com.meizu.settings.location.MzLocationSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_date_time" android:id="@id/date_time_settings" android:title="@string/date_and_time_settings_title" android:fragment="com.android.settings.DateTimeSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_print" android:id="@id/print_settings" android:title="@string/print_settings" android:fragment="com.android.settings.print.PrintSettingsFragment" />
        <dashboard-tile android:icon="@drawable/ic_settings_development" android:id="@id/development_settings" android:title="@string/development_settings_title" android:fragment="com.android.settings.DevelopmentSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_applications" android:id="@id/application_settings" android:title="@string/applications_settings" android:fragment="com.android.settings.applications.ManageApplications" />
        <dashboard-tile android:icon="@drawable/ic_settings_about" android:id="@id/about_settings" android:title="@string/about_settings" android:fragment="com.android.settings.DeviceInfoSettings" />
    </dashboard-category>
</dashboard-categories>
